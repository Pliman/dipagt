# .env.example

# --- PostgreSQL Configuration ---
POSTGRES_DB=dipagt
POSTGRES_USER=user
POSTGRES_PASSWORD=strongpassword # CHANGE THIS
POSTGRES_PORT_HOST=5432 # Optional: Host port mapping for direct DB access

# --- PgAdmin Configuration ---
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=admin # CHANGE THIS
PGADMIN_PORT_HOST=5050 # Host port for accessing PgAdmin web UI

# --- Qdrant Configuration ---
QDRANT_HTTP_PORT_HOST=6334 # Optional: Host port mapping for Qdrant HTTP
QDRANT_GRPC_PORT_HOST=6333 # Optional: Host port mapping for Qdrant gRPC
# QDRANT_API_KEY=your_qdrant_api_key # Uncomment and set if using Qdrant API Key

# --- Server Configuration ---
SERVER_PORT_HOST=8000 # Host port for accessing the backend API directly
# Mem0 API Key (if required by your mem0 setup)
MEM0_API_KEY=your_mem0_api_key
# LLM API Keys and other secrets needed by the server
OPENAI_API_KEY=sk-your-openai-api-key
TAVILY_API_KEY=tvly-your-tavily-api-key
# Add any other necessary environment variables for the server

# --- UI Configuration ---
UI_PORT_HOST=3000 # Host port for accessing the frontend web UI

# --- Docker Compose Specific ---
# Optional: Set the project name prefix for containers/networks/volumes
# COMPOSE_PROJECT_NAME=dipagt_prod 